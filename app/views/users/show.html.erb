<h1><%=@user.username%></h1>


<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th><%= @user.username%>'s  Photos'</th>
        <th>caption</th>
      </tr>

      <% @user.photos.each do |photo| %>
      <tr>

        <td> <img src = "<%= photo.image %>" width = "300"/></td>
        <td><%= photo.caption %></td>

      </tr>
      <% end %>
    </table>
  </div>
</div>
